{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2016",
    "noImplicitAny": false,
    "skipLibCheck": true,
    "sourceMap": true,
    "outDir": "./out",
    "rootDir": "./src",
  }
}